<?xml version="1.0"?>
<block>
  <name>WMbus Preamble</name>
  <key>wmbus_wmbus_preamble</key>
  <category>[Scapy Radio]/WM-Bus</category>
  <import>import wmbus</import>
  <make>wmbus.wmbus_preamble($usleep, $debug)</make>

  <param>
    <name>Sleep (Âµs)</name>
    <key>usleep</key>
    <value>10</value>
    <type>int</type>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>On</name>
      <key>True</key>
    </option>
    <option>
      <name>Off</name>
      <key>False</key>
    </option>
  </param>

  <sink>
    <name>Scapy</name>
    <type>message</type>
  </sink>

  <source>
    <name>out1</name>
    <type>byte</type>
  </source>
  <source>
    <name>out2</name>
    <type>byte</type>
  </source>
  <source>
    <name>out3</name>
    <type>byte</type>
  </source>

</block>
